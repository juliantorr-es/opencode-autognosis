{
  "name": "opencode-autognosis",
  "version": "2.3.1",
  "description": "Advanced RAG-powered codebase awareness for OpenCode agents. Features Chunk Cards synthesis, hierarchical reasoning, ActiveSet working memory, and performance optimization for enterprise-scale repositories.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "assets",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "opencode",
    "rag",
    "code-analysis",
    "chunking",
    "agent",
    "codebase",
    "synthesis",
    "reasoning"
  ],
  "opencode": {
    "type": "plugin",
    "hooks": [],
    "capabilities": [
      "chunk-cards",
      "hierarchical-reasoning",
      "activeset-management",
      "module-summaries",
      "performance-optimization",
      "incremental-indexing",
      "background-processing",
      "memory-management"
    ],
    "compatibility": {
      "min_opencode_version": "1.0.162",
      "node_version": ">=18.0.0"
    }
  },
  "devDependencies": {
    "@opencode-ai/plugin": "^1.0.162",
    "@opencode-ai/sdk": "^1.1.40",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.0.0",
    "bun-types": "^1.3.8",
    "typescript": "^5.0.0",
    "zod": "^4.3.6"
  },
  "dependencies": {
    "better-sqlite3": "^12.6.2",
    "chokidar": "^5.0.0"
  }
}
